{
  "devDependencies": {
    "@types/react": "16.9.53",
    "codecov": "3.8.0",
    "husky": "4.3.0",
    "jest": "26.6.0",
    "lerna": "3.22.1",
    "lint-staged": "10.4.2",
    "oc": "0.48.15",
    "prettier-eslint-cli": "5.0.0"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "coverage": "codecov",
    "clean": "lerna clean",
    "dev": "jest --watch",
    "updated": "lerna updated",
    "test": "jest",
    "publish": "lerna publish --exact",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "packages"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "packages/**/*.js",
      "!packages/coverage/**/*",
      "!node_modules/**",
      "!packages/**/scaffold/**/*",
      "!packages/**/utils/**/*"
    ]
  },
  "dependencies": {}
}
